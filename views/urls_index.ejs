<!DOCTYPE html>
<html lang="en">
<body>

  <% include ./partials/_header %>
  <% if (userid) { %>
    <table>
      <!-- table row -->
      <tr>
        <!-- table heading -->
        <th>Key</th>
        <th>Full URL</th>
      </tr>
    <% for ( let urlKey in urls ) { %>
      <tr>
        <td>
          <form method="POST" action="/urls/<%= urlKey %>/delete">
          <%= urlKey %>
        </td>
        <td>
          <%= urls[urlKey].url + ' ' + urls[urlKey].username %><button method="POST" action="/urls/<%= urlKey %>/delete" type="submit">Delete</button></form>
        </td>
      </tr>
    <% } %>
    </table>
    <!-- <input type="submit" value="Login"> -->
  <a href="/urls/create">Make a new short URL</a>
  <% } else { %>

  <% } %>
</body>
</html>